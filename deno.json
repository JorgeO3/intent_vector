{
  "name": "@intent/vector",
  "version": "0.1.0",
  "exports": {
    ".": "./mod.ts",
    "./core": "./mod.core.ts",
    "./standard": "./mod.standard.ts"
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "test": "deno test tests/ --allow-read --allow-net",
    "bench": "deno bench bench/ --allow-read --allow-net",
    "bench:quick": "deno bench bench/ --allow-read --allow-net --filter 'keyCodec|intentVector'",
    "build": "deno run -A scripts/build.ts",
    "build:check": "deno run -A scripts/bundle-size.ts"
  },
  "imports": {
    "@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.56",
    "@deno-library/compress": "jsr:@deno-library/compress@^0.5.6",
    "@std/assert": "jsr:@std/assert@1"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext", "deno.ns"]
  },
  "lint": {
    "rules": {
      "tags": ["recommended", "jsx", "react", "jsr"]
    }
  }
}
